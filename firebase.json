{
  "firebase": "birdyard",
  "public": "dist",
  "ignore": [
    "firebase.json",
    "**/node_modules/*"
  ],
  "headers": [ {
    "source" : "**/*.@(eot|otf|ttf|ttc|woff|woff2|svg)",
    "headers" : [ {
      "key" : "Access-Control-Allow-Origin",
      "value" : "*"
    } ]
  } ],
  "rewrites": [ {
    "source": "**",
    "destination": "/index.html"
  } ]
}